	<parameter name="inmueble_id" class="java.lang.String"/>
	<queryString>
		<![CDATA[select sum(pa.saldo),u.nombres || ' '  || u.apellidos as nombre
from pagosadelantados pa inner join inmuebles i
   on i.id = pa.inmuebles_id
   inner join contribuyentes c
   on c.id = i.contribuyentes_id
   inner join usuarios u
   on u.id = c.usuarios_id
where inmuebles_id = $P!{inmueble_id}
group by u.nombres || ' '  ||  u.apellidos;]]>
	</queryString>
